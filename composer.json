{
    "name": "contao/manager-plugin",
    "type": "composer-plugin",
    "description": "Contao 4 manager plugin",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Andreas Schempp",
            "homepage": "https://github.com/aschempp"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "composer-plugin-api": "^1.1",
        "symfony/config": "^3.3",
        "symfony/dependency-injection": "^3.3",
        "symfony/filesystem": "^3.3",
        "symfony/http-kernel": "^3.3",
        "symfony/routing": "^3.3"
    },
    "require-dev": {
        "composer/composer": "^1.1",
        "contao/core-bundle": "^4.3.3",
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^5.7",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Contao\\ManagerPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Contao\\ManagerPlugin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "2.2.x-dev"
        },
        "class": "Contao\\ManagerPlugin\\Composer\\Plugin"
    }
}
